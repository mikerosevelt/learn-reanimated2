{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "moduleResolution": "node",
    "lib": ["DOM", "ESNext"],
    "jsx": "react-native",
    "allowJs": false
  },
  "extends": "expo/tsconfig.base"
}
